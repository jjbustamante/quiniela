 
<%= form_for @quiniela, url: '/quinielas'  do |f| %>
            <div class="page-header">
            <h1 id="tables">Crea tu Quiniela</h1>
            </div>
               <h3 id="tables">Nombre:</h3><%= f.text_field :name, :class => "form-control", :maxlength => "35" %>
               <div class="page-header">
            <h2 id="tables">Fase de grupos</h2>
            </div>
                    <%= f.fields_for :bets do |builder| %>
                      <%= render :partial => 'bets_fields', :locals => {:f => builder} %>
                    <% end %>

              <p>
                <%= f.submit %>
              </p>
              <% end %>
