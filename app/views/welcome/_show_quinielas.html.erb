 <div class="page-header">
              <h1 id="tables">Mis quinielas</h1>
            </div>



<% if @quinielas.count > 0 %>
<table class="table table-striped table-hover ranking-table">
              <tr>
                  
                  <td colspan="1">Numero de quiniela</td>
                  <td>Nombre de quiniela</td>
                  <td>Puntos acumulados</td>
                  <td colspan="2">Accion</td>
                </tr> 
           
            <%
     
                @var1 =  ''
                @quinielas.each do |quiniela| %>
             
                <tr>
                  <td><span class="stuart-middle"># <%= quiniela.id %></span></td>
                  <td valign="middle"><span class="stuart-middle"><%= quiniela.name %></span></td>
                  <td><span class="stuart-middle"><%= quiniela.points %></span></td>
                  <td width="88px"><a href="#" class="btn btn-primary">Editar</a></td>
                   <td width="88px"><a href="#" class="btn btn-danger">Eliminar</a></td>
                  
                </tr> 
<% end 

  %>
                          
            </table>

      <% 
else
  %>
<p class="lead welcome-message">¿Aun no comienzas? Crea tu primera quiniela haciendo click <a href="/quinielas/new">AQUÍ</a></p>
  <%
 end%>
