                    <% end %>
                    <%= match.team1.name %>
                    </div>
                  </td>
                  <td class="centered">

<% if @played %>
                    <table style="height: 32px;">
                      <tbody>
                        <tr>
                          <td id="match_<%= match.id %>_score_1" t1="<%= match.team1.id %>" class="played matcht1id_<%= match.id %>" ><%= match.score_t1 %></td>
                          <td><div class="form-separator">-</div></td>
                          <td id="match_<%= match.id %>_score_2" winner_id="<%= match.winner_id %>" t2="<%= match.team2.id %>" class="matcht2id_<%= match.id %>"><%= match.score_t2 %></td></tr>
                    </tbody></table>
<% else%>
<div style="height: 32px;">
<%= match.match_date.to_formatted_s(:time) %>
</div>
<% end%>
 


                 

                  </td>
                  <td class="rigth-align col-ajust">
                     <div class="name-wrapper right">
