<% end  %>




<table class="table table-striped table-hover 
<% if @played %>
game-over
<% end%>

 ">
              <tbody>
              

                <tr>
                  <td style="position:relative;">
                    <div style="position:relative;">
                    <div id="earned_<%= match.id %>" class="points"></div><span class="flagsp flagsp_<%= match.team1.code %>" title=""> </span>
                  </div>
                  </td>
                  <td class="col-ajust">
                   
                    <div class="name-wrapper left">
