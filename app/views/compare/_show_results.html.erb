 <div id="round1" class="page-header">
              <h1 id="tables" >Resultados reales</h1>
            </div>
            
              
               <%
                @var1 =  ''
                
                @matches.each do |match|
                    @played = false
                    @winner_id = match.winner_id
                  if match.played == 1 
                    @played = true
                  end 
                
              
                if match.match_date.strftime("%m/%d") != @var1 %>
                
